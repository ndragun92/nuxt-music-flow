{
  "name": "nuxt-music-flow",
  "version": "1.1.2",
  "description": "Nuxt 3 module for audio player component with playlist and waveform visualization.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndragun92/nuxt-music-flow.git"
  },
  "bugs": {
    "url": "https://github.com/ndragun92/nuxt-music-flow/issues"
  },
  "license": "MIT",
  "type": "module",
  "homepage": "https://vue-music-flow-docs.vercel.app/",
  "keywords": [
    "nuxt3",
    "module",
    "audio player",
    "music player",
    "playlist",
    "waveform",
    "component"
  ],
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.15.4",
    "vue-music-flow": "^0.2.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.1.3",
    "@nuxt/eslint-config": "^1.1.0",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.17.1",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.21.0",
    "nuxi": "^3.23.0",
    "nuxt": "^3.15.4",
    "typescript": "~5.8.2",
    "vitest": "^3.0.7",
    "vue-tsc": "^2.2.8"
  }
}
